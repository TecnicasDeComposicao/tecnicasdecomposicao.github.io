=\"1\" step=\"0.1\" value=\"0.6\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n    <!-- End block parameters -->\n    </mbr-parameters>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay &amp;&amp; bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n    <div class=\"container countdown-cont align-center\">\n        <div class=\"row\">\n          <div class=\"col-xs-3 col-sm-3 col-md-3\">\n            <span class=\"number-wrap\">\n              <span class=\"number display-2\">00</span>\n              <span mbr-text=\"\" class=\"period display-7\">Dias</span>\n              <span class=\"dot\">:</span>\n            </span>\n          </div>\n          <div class=\"col-xs-3 col-sm-3 col-md-3\">\n            <span class=\"number-wrap\">\n              <span class=\"number display-2\">00</span>\n              <span mbr-text=\"\" class=\"period display-7\">Horas</span>\n              <span class=\"dot\">:</span>\n            </span>\n          </div>\n          <div class=\"col-xs-3 col-sm-3 col-md-3\">\n            <span class=\"number-wrap\">\n              <span class=\"number display-2\" id=\"min\">21</span>\n              <span mbr-text=\"\" class=\"period display-7\">Minutos</span>\n              <span class=\"dot\">:</span>\n            </span>\n          </div>\n          <div class=\"col-xs-3 col-sm-3 col-md-3\">\n            <span class=\"number-wrap\">\n              <span class=\"number display-2\" id=\"seg\">23</span>\n              <span mbr-text=\"\" class=\"period display-7\">Segundos</span>\n            </span>\n          </div>\n        </div>\n    </div>\n    <!-- Contador -->\n<script>\n    var minutes = 21;\n    var seconds = 24;\n    var countdown = setInterval(function() {\n        seconds--;\n        if (seconds<0 && minutes>0) {\n            seconds = 59;\n            minutes--;\n           document.getElementById(\"min\").innerHTML = minutes;\n        };\n        if (seconds<10)\n           document.getElementById(\"seg\").innerHTML = \"0\" + seconds\n       else\n           document.getElementById(\"seg\").innerHTML = seconds;\n        if (seconds<1 && minutes<1) {\n           document.getElementById(\"min\").innerHTML = \"00\";\n           document.getElementById(\"seg\").innerHTML = \"00\";\n           this.clearinterval();\n        };\n    }, 1000);\n</script>\n</section>",
